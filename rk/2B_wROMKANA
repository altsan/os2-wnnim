(defvar escd (list - , . / '\;' : [ '\\' ] '\^' '\~'))
X(escd)	(escd)  

(defvar chouon (list '\^' '\~'))
(last= A)(chouon)	丐
(last= I)(chouon)	中
(last= U)(chouon)	丹
(last= E)(chouon)	中
(last= O)(chouon)	丹

(defvar alpha (between A Z))
(alpha)	(error)
